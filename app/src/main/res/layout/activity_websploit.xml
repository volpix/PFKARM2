<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/f2"
    android:orientation="vertical"
    tools:context=".Autenticacion">

    <Button
        android:id="@+id/button12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:background="@color/colorPrimaryDark"
        android:onClick="pantalla5"
        android:text="@string/atras"
        android:textColor="@color/colorAccent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.026"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.011" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/w"
                android:layout_width="wrap_content"
                android:layout_height="211dp"
                android:layout_gravity="center"
                android:layout_marginBottom="4dp"
                android:contentDescription="w"
                app:srcCompat="@drawable/w" />

            <ImageView
                android:id="@+id/w2"
                android:layout_width="match_parent"
                android:layout_height="209dp"
                android:layout_gravity="center"
                android:layout_marginBottom="4dp"
                android:contentDescription="w2"
                app:srcCompat="@drawable/w2" />

            <ImageView
                android:id="@+id/w3"
                android:layout_width="wrap_content"
                android:layout_height="244dp"
                android:layout_gravity="center"
                android:layout_marginBottom="4dp"
                android:contentDescription="w3"
                app:srcCompat="@drawable/w3" />

            <ImageView
                android:id="@+id/w4"
                android:layout_width="wrap_content"
                android:layout_height="249dp"
                android:layout_gravity="center"
                android:layout_marginBottom="4dp"
                android:contentDescription="w4"
                app:srcCompat="@drawable/w4" />

            <ImageView
                android:id="@+id/w5"
                android:layout_width="wrap_content"
                android:layout_height="130dp"
                android:layout_gravity="center"
                android:layout_marginBottom="4dp"
                android:contentDescription="w5"
                app:srcCompat="@drawable/w5" />

        </LinearLayout>

    </ScrollView>


</LinearLayout>

